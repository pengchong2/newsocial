<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tl="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/car_light_tv"
        style="@style/left_fragment_text_style"
        android:layout_marginTop="88px"
        android:layout_marginStart="41px"
        android:text="外部车灯"
        />
    <com.flyco.tablayout.CommonTabLayout
        android:layout_marginStart="48px"
        android:layout_marginTop="133px"
        android:id="@+id/commonTabLayout_view2"
        android:layout_width="982px"
        android:layout_height="87px"
        android:background="@drawable/commonyablayout_view7_bg"
        tl:tl_iconVisible="false"
        tl:tl_indicator_style="BLOCK"
        tl:tl_indicator_bounce_enable="false"
        tl:tl_indicator_color="#E5512F"
        tl:tl_indicator_height="74px"
        tl:tl_indicator_margin_top="2px"
        tl:tl_indicator_margin_bottom="8px"
        tl:tl_indicator_margin_left="8px"
        tl:tl_indicator_margin_right="8px"
        tl:tl_textsize="24px"/>
    <ImageView
        android:src="@drawable/commonyablayout_view8_data"
        android:layout_marginStart="48px"
        android:layout_marginTop="133px"
        android:elevation="2dp"
        android:layout_width="982px"
        android:layout_height="87px"/>

    <cn.flyaudio.leftfragment.view.MySwitch
        android:visibility="gone"
        android:id="@+id/car_light_switch"
        android:thumb="@null"
        android:textColor="#FFFFFF"
        android:textSize="24px"
        android:paddingBottom="5px"
        android:gravity="center"
        android:background="@drawable/checkbox_select_bg"
        android:elevation="2dp"
        android:text="车灯关闭"
        android:layout_marginTop="133px"
        android:layout_marginStart="839px"
        android:layout_width="193px"
        android:layout_height="87px" />


    <TextView
        style="@style/left_fragment_text_style"
        android:layout_marginTop="268px"
        android:layout_marginStart="41px"
        android:text="伴我回家灯（下车后大灯延迟熄灭）"
        />
    <com.flyco.tablayout.CommonTabLayout
        android:layout_marginStart="48px"
        android:layout_marginTop="315px"
        android:id="@+id/commonTabLayout_view3"
        android:layout_width="384px"
        android:layout_height="87px"
        android:background="@drawable/commonyablayout_view4_bg"
        tl:tl_iconVisible="false"
        tl:tl_indicator_style="BLOCK"
        tl:tl_indicator_bounce_enable="false"
        tl:tl_indicator_color="#E5512F"
        tl:tl_indicator_height="74px"
        tl:tl_indicator_margin_top="2px"
        tl:tl_indicator_margin_bottom="8px"
        tl:tl_indicator_margin_left="8px"
        tl:tl_indicator_margin_right="8px"
        android:paddingBottom="8px"
        tl:tl_textsize="24px"/>

    <TextView
        style="@style/left_fragment_text_style"
        android:layout_marginTop="448px"
        android:layout_marginStart="41px"
        android:text="左右转向灯"
        />
    <com.flyco.tablayout.CommonTabLayout
        android:layout_marginStart="48px"
        android:layout_marginTop="495px"
        android:id="@+id/commonTabLayout_view4"
        android:layout_width="577px"
        android:layout_height="87px"
        android:background="@drawable/commonyablayout_view5_bg"
        tl:tl_iconVisible="false"
        tl:tl_indicator_style="BLOCK"
        tl:tl_indicator_bounce_enable="false"
        tl:tl_indicator_color="#E5512F"
        tl:tl_indicator_height="74px"
        tl:tl_indicator_margin_top="2px"
        tl:tl_indicator_margin_bottom="8px"
        tl:tl_indicator_margin_left="8px"
        tl:tl_indicator_margin_right="8px"
        tl:tl_textsize="24px"/>
    <ImageView
        android:src="@drawable/commonyablayout_view4_data"
        android:layout_marginStart="48px"
        android:layout_marginTop="495px"
        android:elevation="2dp"
        android:layout_width="577px"
        android:layout_height="87px"/>

    <View
        android:background="#425269"
        android:layout_marginStart="40px"
        android:layout_marginEnd="40px"
        android:layout_marginTop="628px"
        android:layout_width="match_parent"
        android:layout_height="2px"/>

    <ListView
        android:id="@+id/left_fragment_listview"
        android:padding="0dp"
        android:divider="@null"
        android:layout_marginTop="679px"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </ListView>


</FrameLayout>