<?xml version="1.0" encoding="utf-8"?>
<com.yxf.clippathlayout.impl.ClipPathFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <!--<View
        android:id="@+id/center_view"
        android:layout_width="54px"
        android:layout_height="54px"
        android:layout_gravity="center"
        android:background="#009688"
        android:clickable="true" />-->
    <ImageView
        android:id="@+id/left_fragment_hsj_iv"
        android:layout_gravity="center"
        android:src="@drawable/left_fragment_hsj_bg"
        android:layout_width="175px"
        android:layout_height="175px" />

    <View
        android:id="@+id/car_hsj_left_view"
        android:layout_width="176px"
        android:layout_height="176px"
        android:layout_gravity="center"
        android:background="@drawable/left_fragment_hsj_left"
        android:clickable="true" />

    <View
        android:id="@+id/car_hsj_right_view"
        android:layout_width="176px"
        android:layout_height="176px"
        android:layout_gravity="center"
        android:background="@drawable/left_fragment_hsj_right"
        android:clickable="true" />

    <View
        android:id="@+id/car_hsj_top_view"
        android:layout_width="176px"
        android:layout_height="176px"
        android:layout_gravity="center"
        android:background="@drawable/left_fragment_hsj_top"
        android:clickable="true" />

    <View
        android:id="@+id/car_hsj_bottom_view"
        android:layout_width="176px"
        android:layout_height="176px"
        android:layout_gravity="center"
        android:background="@drawable/left_fragment_hsj_bottm"
        android:clickable="true" />


</com.yxf.clippathlayout.impl.ClipPathFrameLayout>